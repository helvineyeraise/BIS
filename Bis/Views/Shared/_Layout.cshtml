<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - bis</title>
    <!-- Global stylesheets -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/global_assets/css/icons/icomoon/styles.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/assets/css/bootstrap_limitless.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/assets/css/layout.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/assets/css/components.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/assets/css/colors.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/assets/css/custom.css")" rel="stylesheet" />

    <!-- Core JS files -->
    <script src="@Url.Content("~/global_assets/js/main/jquery.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/main/bootstrap.bundle.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/loaders/blockui.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/ui/slinky.min.js")"></script>

    <!-- Theme JS files -->
    <script src="@Url.Content("~/global_assets/js/plugins/visualization/d3/d3.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/visualization/d3/d3_tooltip.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/forms/styling/switchery.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/ui/moment/moment.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/pickers/daterangepicker.js")"></script>
    <!-- /theme JS files -->
    <!-- Common -->
    <link href="@Url.Content("~/global_assets/css/icons/icomoon/styles.min.css")" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/global_assets/css/icons/material/styles.min.css")" rel="stylesheet" type="text/css">
    <script src="@Url.Content("~/global_assets/js/plugins/tables/datatables/datatables.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/forms/selects/select2.min.js")"></script>
    <script src="@Url.Content("~/assets/js/custom.js")"></script>
    <script src="@Url.Content("~/assets/js/app.js")"></script>
    <!-- Common -->
    <!-- Date Picker -->
    <script src="@Url.Content("~/global_assets/js/plugins/pickers/pickadate/picker.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/pickers/pickadate/picker.date.js")"></script>
    <script src="@Url.Content("~/assets/js/picker_date.js")"></script>
    <script src="@Url.Content("~/assets/js/jquery.validate.js")"></script>
    <!-- Date Picker -->
    <!-- Form Horizontal-->
    <script src="@Url.Content("~/global_assets/js/plugins/forms/selects/select2.min.js")"></script>
    <script src="@Url.Content("~/global_assets/js/plugins/forms/styling/uniform.min.js")"></script>
    <!-- Form Horizontal-->
    <script src="@Url.Content("~/global_assets/js/plugins/notifications/bootbox.min.js")"></script>
   
</head>
<body>

    <!-- Main navbar -->
    <div class="navbar navbar-expand-md navbar-dark">
        <div class="wmin-0 mr-5">
            <a href="index.html" class="d-inline-block">
                <img style="height:40px; padding-top:5px;" src="~/assets/img/bis-logo.png" />
            </a>
        </div>

        <div class="d-md-none">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar-mobile">
                <i class="icon-tree5"></i>
            </button>
        </div>

        <div class="collapse navbar-collapse" id="navbar-mobile">
            <span class="ml-md-3 mr-md-auto" style="font-size:12pt;">Bright Inspection Service</span>


            <ul class="navbar-nav">
                <li class="nav-item dropdown dropdown-user">
                    <a href="#" class="navbar-nav-link d-flex align-items-center dropdown-toggle" data-toggle="dropdown">
                        <span>@Bis.Custom.Display.Name</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <a href="#" class="dropdown-item"><i class="icon-user-plus"></i>@Bis.Custom.Display.Role</a>
                        <a href="@Url.Content("~/Auth/Logout")" class="dropdown-item"><i class="icon-switch2"></i> Logout</a>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Secondary navbar -->
    <div class="navbar navbar-expand-md navbar-light">
        <div class="text-center d-md-none w-100">
            <button type="button" class="navbar-toggler dropdown-toggle" data-toggle="collapse" data-target="#navbar-navigation">
                <i class="icon-unfold mr-2"></i>
                Navigation
            </button>
        </div>
        <div class="navbar-collapse collapse" id="navbar-navigation">
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a href="~/Home/Index" class="navbar-nav-link active">
                        <i class="icon-home5 mr-2"></i>
                        Home
                    </a>
                </li>
                <li class="nav-item dropdown">
                    <a href="#" class="navbar-nav-link dropdown-toggle" data-toggle="dropdown">
                        <i class="icon-user"></i>
                        Admin
                    </a>
                    <div class="dropdown-menu">
                        <a href="~/Users/Index" class="dropdown-item">Users</a>
                        <a href="~/Locations/Index" class="dropdown-item"> Location</a>
                        <a href="~/Categories/Index" class="dropdown-item"> Category</a>
                        <a href="~/SubCategories/Index" class="dropdown-item"> Sub Category</a>
                        <a href="~/Departments/Index" class="dropdown-item"> Department</a>
                        <a href="~/CompanyCategories/Index" class="dropdown-item">Company Category</a>
                        <a href="~/Vendors/Index" class="dropdown-item">Vendor</a>
                        <a href="~/Charges/Index" class="dropdown-item">Charge</a>
                        <a href="~/Settings/Index" class="dropdown-item">Settings</a>
                    </div>
                </li>
                <li class="nav-item">
                    <a href="~/Companies/Index" class="navbar-nav-link">
                        <i class="icon-city mr-2"></i>
                        Company
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/Employees/Index" class="navbar-nav-link">
                        <i class="icon-user mr-2"></i>
                        Employee
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/TPIAllocations/Index" class="navbar-nav-link">
                        <i class="icon-phone-wave mr-2"></i>
                        TPI Allocation
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/TPICalls/Index" class="navbar-nav-link">
                        <i class="icon-phone-wave mr-2"></i>
                        TPI Calls
                    </a>
                </li>
                <li class="nav-item">
                    <a href="~/Attendances/Index" class="navbar-nav-link">
                        <i class="icon-user-check mr-2"></i>
                        Attendance
                    </a>
                </li>
                <li class="nav-item dropdown">
                    <a href="#" class="navbar-nav-link dropdown-toggle" data-toggle="dropdown">
                        <i class="icon-coin-dollar"></i>
                        Payroll
                    </a>
                    <div class="dropdown-menu">
                        <a href="~/Advances/Index" class="dropdown-item">Advance</a>
                        <a href="~/Loans/Index" class="dropdown-item">Loan</a>
                        <a href="~/Detections/Index" class="dropdown-item">Deduction</a>
                        <a href="~/Reports/Salary" class="dropdown-item">Salary Process</a>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a href="#" class="navbar-nav-link dropdown-toggle" data-toggle="dropdown">
                        <i class="icon-coin-dollar"></i>
                        Reports
                    </a>
                    <div class="dropdown-menu">
                        <a href="~/Reports/AttendanceConsolidate" class="dropdown-item">Attendance Consolidate</a>
                        <a href="~/Reports/DailyAttendance" class="dropdown-item">Daily Attendance</a>
                        <a href="~/Reports/Advance" class="dropdown-item">Advance</a>
                        <a href="~/Reports/Loan" class="dropdown-item">Loan</a>
                        <a href="~/Reports/Detection" class="dropdown-item">Deduction</a>
                        <a href="~/Reports/TravelExpenses" class="dropdown-item">Travel Expenses</a>
                        <a href="~/Reports/IdCard" class="dropdown-item">ID Card</a>
                        <a href="~/Reports/EmployeeDetails" class="dropdown-item">Employee List</a>
                        <a href="~/Reports/EmpDetails" class="dropdown-item">Employee Details</a>
                        <a href="~/Reports/ExperienceCertificate" class="dropdown-item">Experience Certificate</a>
                        <a href="~/Reports/MoneyTransfer" class="dropdown-item">Instant Money Details</a>
                        <a href="~/Reports/EsiPf" class="dropdown-item">ESI Report</a>
                        <a href="~/Reports/PaySlip" class="dropdown-item">Pay Slip</a>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- /secondary navbar -->
    <!-- Page content -->
    <div class="page-content pt-0">
        <!-- Main content -->
        <div class="content-wrapper">
            <!-- Content area -->
            <div class="content">
                <br />
                @RenderBody()
            </div>
            <!-- /content area -->
        </div>
        <!-- /main content -->
    </div>
    <!-- /page content -->
    <!-- Footer -->
    <div class="navbar navbar-expand-lg navbar-light">
        <div class="text-center d-lg-none w-100">
            <button type="button" class="navbar-toggler dropdown-toggle" data-toggle="collapse" data-target="#navbar-footer">
                <i class="icon-unfold mr-2"></i>
                Footer
            </button>
        </div>

        <div class="navbar-collapse collapse" id="navbar-footer">
            <span class="navbar-text">
                2020 @@ <a href="http://eyeraise.com" target="_blank">eyeraise</a>
            </span>

            <ul class="navbar-nav ml-lg-auto">
                <li class="nav-item"><a href="https://kopyov.ticksy.com/" class="navbar-nav-link" target="_blank"><i class="icon-lifebuoy mr-2"></i> Support</a></li>
                <li class="nav-item"><a href="http://demo.interface.club/limitless/docs/" class="navbar-nav-link" target="_blank"><i class="icon-file-text2 mr-2"></i> Docs</a></li>
            </ul>
        </div>
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
